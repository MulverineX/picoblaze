{
  "schemaVersion": 1,
  "id": "picoblaze",
  "version": "${version}",
  "name": "Picoblaze",
  "description": "A collection of experimental features often discussed, to test if they would truly benefit commands and mapmaking.",
  "authors": [
    "lolad"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/replace/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.lolad.picoblaze.PicoBlaze"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.lolad.picoblaze.PicoblazeClient"
      }
    ]
  },
  "mixins": [
    "picoblaze.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "fabric-language-kotlin": ">=1.6.5+kotlin.1.5.31"
  }
}
